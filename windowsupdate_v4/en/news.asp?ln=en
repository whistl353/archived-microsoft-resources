<script language ="javascript" >
	var g_bNewsError = false ;
	function window.onload() {
		var oReadAlls , iLen, i;
		document.ondragstart = new Function("return false;");
		document.oncontextmenu = new Function("return false;");
		if (!g_bNewsError) {
			window.parent.document.all("newsframe").style.display = "block" ;  
			oReadAlls = document.getElementsByName("ReadMore");
			iLen = oReadAlls.length ;
			for (i = 0; i < iLen; i++) oReadAlls[i].innerText = window.parent.parent.L_ReadMore_Text ;
		}	
	}
</script>		
