
	<html>
	<head>
	<script language="javascript">
		function fnGetLang() {
			var i, sURL, sQuery;
			var ln = navigator.browserLanguage;
			if(ln == "" || ln == null) ln = "en";
			sURL = "default.aspx?ln=" + ln;
			sQuery = window.location.href;
			i = sQuery.indexOf("?");
			if(i != -1) {
				sQuery = "&" + sQuery.substring(i + 1)
				sURL += sQuery;
			}
			window.location.replace(sURL);
		}
		
		function fnGetQueryString(sURL, sKey){
			var vValue;
			vValue = sURL.match("(\\?|&)" + sKey + "=([^&$]*)");
			vValue = (vValue == null || vValue.length == 1) ? "" : vValue[2];
			return vValue;
		}
	</script>
