

		<script language="JScript" type="text/javascript">
			function fnPingback() {
				var errnumber = 0x0;
				var queryString;
				var pingbackURL = "http://supportstats.windowsupdate.com/v5track.bin" 
				queryString = "?v5_b1_1367_err=0x" + errnumber; 
				pingback.location.replace(pingbackURL + queryString);
			}
		</script>


