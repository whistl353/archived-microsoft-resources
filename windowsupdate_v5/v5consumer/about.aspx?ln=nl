						<td  class="FAQtd1">X</td>
						<td  class="FAQtd1">X</td>
						<td  class="FAQtd2">X</td>
					</tr>
					<tr >
						<td  class="FAQtd1">Updatepakketten</td>
						<td  class="FAQtd1">X</td>
						<td  class="FAQtd1">X</td>
						<td  class="FAQtd1">X</td>
						<td  class="FAQtd2">X</td>
					</tr>
