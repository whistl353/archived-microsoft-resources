<script language="vbscript">
Function vbsDateAdd(iIntervel,sDate)
	If Not IsDate(sDate) Then
		vbsDateAdd = ""
	Else
		vbsDateAdd = DateAdd("n",iIntervel,sDate)
	End If
End Function
