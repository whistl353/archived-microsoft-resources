	<script language="javascript">
		var iSubPage, sUpdates;
		var iTocIndex;
	
		function InitResults() 
		{				
			iSubPage = 2;
			sUpdates = "";
			iTocIndex = "";
			//fnSetResultsPageVariables(sUpdates != "");
			fnWriteUpdates(sUpdates,true);
			var SubPage = 2
			if(iSubPage == parent.conExpressInstall) SubPage = parent.conResultsBasket;			
			fnInit(2, SubPage);
		}
		
	</script>
