var g_bSPDetectOn = true; // turn SP detection  
var spUpdateIds = new Array();
fnInitSPArray();

function fnInitSPArray(){
	
	if (g_bSPDetectOn){
		spUpdateIds[0] = "fc420a3e-f41f-4d17-b0d5-907558d01884";
	}
	else {
		spUpdateIds.length = 0 ;
	}	
}	
