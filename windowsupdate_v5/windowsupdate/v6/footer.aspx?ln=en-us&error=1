<script language="javascript" defer="true">
		function fnSetLinksTarget(){
			try{
				var conQueryString = "ln=en-us"; 
				conQueryString = conQueryString.replace("&amp;","&");
				
				if(typeof(document.all("msviLocalFooter")) == "object"){ 
					var sLang = "en" ;
					var sConsumerURL = "/windowsupdate/v6/";//got here rather than from parent is because of muopt page.
					
					if(sConsumerURL == "/microsoftupdate/v6/" || sConsumerURL == "/windowsupdate/v6/"){   // this is because due to exception key, "manage your profile" link is removed 
						document.all("msviLocalFooter").childNodes(0).childNodes(0).target = "eContent";
						document.all("msviLocalFooter").childNodes(0).childNodes(0).setAttribute("href","privacy.aspx?" + conQueryString ) ;
					}
					else {
						document.all("msviLocalFooter").childNodes(2).childNodes(0).target = "eContent";
						document.all("msviLocalFooter").childNodes(2).childNodes(0).setAttribute("href","privacy.aspx?" + conQueryString ) ;
					}	
				}	   
			}
			catch(e){}	 
		}
</script>
