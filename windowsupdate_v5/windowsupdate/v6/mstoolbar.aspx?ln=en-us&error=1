		function fnAdjustLocalToolbar(){ 
			try { 
				var oSeparatorNode = document.all("msviHomePageLink").parentNode.childNodes(1).childNodes(0);
				if(typeof(oSeparatorNode) == "object"){
					oSeparatorNode.style.display = "none";
